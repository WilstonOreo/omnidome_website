---
title: Home page
listed: false
published: true
cache_strategy: none
response_type: text/html
position: 0
editable_elements:
  "/Analytics": "<script>\r\n(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){\r\n(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new
    Date();a=s.createElement(o),\r\nm=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)\r\n})(window,document,'script','//www.google-analytics.com/analytics.js','ga');\r\n\r\nga('create',
    'UA-49334851-1', 'omnido.me');\r\nga('send', 'pageview');\r\n</script>"
---
<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- META DATA -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">

    {% seo %}

    <link rel="canonical" href="http://www.omnido.me/{{page.fullpath}}" />
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ 'ico/apple-touch-icon-144-precomposed.png' | theme_image_url }}">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ 'ico/apple-touch-icon-72-precomposed.png' | theme_image_url }}">
    <link rel="apple-touch-icon-precomposed" href="{{ 'ico/apple-touch-icon-57-precomposed.png' | theme_image_url }}">
    <link rel="shortcut icon" href="{{ 'ico/favicon.png' | theme_image_url }}">

    <!-- STYLESHEETS -->
    {{ 'theme.css' | stylesheet_tag: 'screen' }}

    <!-- GOOGLE WEB FONTS -->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,700,600,300,800' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Roboto:900' rel='stylesheet' type='text/css'>
  </head>

  <body>
    {% include header %}

    {% block content %}
      {% include welcome %}

      {% include portfolio %}

      {% include services %}

      {% include features %}

      {% include faq %}

      {% include about %}

      {% include twitter %}

      {% include contact %}

      {% include footer %}
    {% endblock %}


    <!--[if lt IE 9]>
      {{ 'html5shiv.js' | javascript_tag }}
    <![endif]-->
    <!-- All libraries below in one minified js file -->
    {{ 'all.min.js' | javascript_tag }}

    <!-- Unminified dev libraries
      {{ 'jquery-1.10.2.min.js' | javascript_tag }}
      {{ 'bootstrap.js' | javascript_tag }}
      {{ 'plugins/jquery.bxslider.js' | javascript_tag }}
      {{ 'plugins/jquery.centralized.js' | javascript_tag }}
      {{ 'plugins/jquery.fixedonlater.js' | javascript_tag }}
      {{ 'plugins/jquery.hashloader.js' | javascript_tag }}
      {{ 'plugins/jquery.mixitup.js' | javascript_tag }}
      {{ 'plugins/jquery.nav.js' | javascript_tag }}
      {{ 'plugins/jquery.parallax-1.1.3.js' | javascript_tag }}
      {{ 'plugins/jquery.fitvids.js' | javascript_tag }}
      {{ 'plugins/jquery.scrollTo.js' | javascript_tag }}
      {{ 'plugins/twitterFetcher_v10_min.js' | javascript_tag }}
      {{ 'plugins/jquery.form.min.js' | javascript_tag }}
      {{ 'application.js' | javascript_tag }}
    -->

    {% editable_text "Analytics", hint: "Googlemutherfuckinganalytics", fixed: true, format: raw, rows: 12 %}
    {% endeditable_text %}
  </body>
</html>
